{
    "multipart": [
        {   "apply": { "model": "fossil:ancient_glass" }},

        {   "when": { "up": "false" },
            "apply": { "model": "fossil:ancient_glass_down", "uvlock": false }
        },
        {   "when": { "down": "false" },
            "apply": { "model": "fossil:ancient_glass_up", "uvlock": false }
        },
        {   "when": { "east": "false" },
            "apply": { "model": "fossil:ancient_glass_left", "y" : 0, "uvlock": false }
        },
        {   "when": { "west": "false" },
            "apply": { "model": "fossil:ancient_glass_right", "y" : 0, "uvlock": false }
        },
        {   "when": { "north": "false" },
            "apply": { "model": "fossil:ancient_glass_right", "y" : 90, "uvlock": false }
        },
        {   "when": { "south": "false" },
            "apply": { "model": "fossil:ancient_glass_left", "y" : 90, "uvlock": false }
        },
        {   "when": { "east": "false", "up": "false", "north": "false" },
            "apply": { "model": "fossil:ancient_glass_top_right", "y" : 0, "uvlock": false }
        },
        {   "when": { "east": "false", "up": "false", "south": "false" },
            "apply": { "model": "fossil:ancient_glass_top_left", "y" : 180, "uvlock": false }
        },
        {   "when": { "east": "false", "down": "false" },
            "apply": { "model": "fossil:ancient_glass_bottom_right", "y" : 0, "uvlock": false }
        },
        {   "when": { "west": "false", "down": "false" },
            "apply": { "model": "fossil:ancient_glass_bottom_left", "y" : 0, "uvlock": false }
        },
        {   "when": { "north": "false", "up": "false", "west": "false" },
            "apply": { "model": "fossil:ancient_glass_top_right_ns", "y" : -90, "uvlock": false }
        },
        {   "when": { "south": "false", "up": "false", "west": "false" },
            "apply": { "model": "fossil:ancient_glass_top_left_ns", "y" : -90, "uvlock": false }
        },
        {   "when": { "north": "false", "down": "false", "east": "false" },
            "apply": { "model": "fossil:ancient_glass_bottom_right", "y" : -90, "uvlock": false }
        },
        {   "when": { "south": "false", "down": "false", "east": "false" },
            "apply": { "model": "fossil:ancient_glass_bottom_left", "y" : -90, "uvlock": false }
        }
    ]
}